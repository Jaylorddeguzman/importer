# MongoDB Connection (Required)
MONGODB_URI=your_mongodb_connection_string

# Deployment URL for self-ping keep-alive (Required for Render)
# Set this to your Render service URL after deployment
# Example: https://osm-importer-service.onrender.com
RENDER_EXTERNAL_URL=YOUR_RENDER_URL

# Import Configuration (Optional)
IMPORT_MODE=continuous
IMPORT_DELAY=3000

# Port (Optional - Render will set this automatically)
PORT=3001
